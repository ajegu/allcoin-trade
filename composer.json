{
  "name": "ajegu/allcoin-trade",
  "description": "Allcoin is a crypto-currency project.",
  "keywords": ["framework", "laravel", "lumen", "crypto", "bitcoin"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.0",
    "ajegu/allcoin-core": "dev-alc-70",
    "ajegu/binance-sdk": "^1",
    "aws/aws-sdk-php": "^3.178",
    "bref/bref": "^1.2",
    "laravel/lumen-framework": "^8.0",
    "nyholm/psr7": "^1.4",
    "php-http/guzzle7-adapter": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "psr-4": {
      "AllCoinTrade\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/ajegu/binance-sdk"
    },
    {
      "type": "github",
      "url": "https://github.com/ajegu/allcoin-core"
    }
  ]
}
